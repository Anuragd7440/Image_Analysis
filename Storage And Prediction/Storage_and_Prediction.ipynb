        "    '% Work Done': min(previous_work_done + random.randint(2,7), 100)       # max 100%\n",
        "}\n",
        "\n",
        "new_row_df = pd.DataFrame([new_row_data])\n",
        "df = pd.concat([df, new_row_df], ignore_index=True)"
